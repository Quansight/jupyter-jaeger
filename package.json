{
  "name": "jupyter-jaeger",
  "version": "1.0.0",
  "description": "A library for saving traces to jaeger",
  "keywords": [
    "jupyter",
    "jaeger"
  ],
  "homepage": "https://github.com/Quansight/jupyter-jaeger",
  "bugs": {
    "url": "https://github.com/Quansight/jupyter-jaeger/issues"
  },
  "license": "MIT",
  "author": "Saul Shanabrook",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Quansight/jupyter-jaeger.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepublish": "yarn run clean && yarn run build",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/coreutils": "3.1.0",
    "jaeger-browser": "0.1.0"
  },
  "devDependencies": {
    "prettier": "^1.17.0",
    "rimraf": "3.0.0",
    "typescript": "~3.5.2"
  }
}
